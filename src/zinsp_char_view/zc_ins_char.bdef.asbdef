projection;
strict ( 2 );
use draft;

define behavior for ZC_INS_CHAR //alias <alias_name>
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Activate;
  use action Discard;
  use action Resume;
  use action Prepare;

  use association _Root { create; with draft; }
}

define behavior for ZC_INS_CHAR_ITEM //alias <alias_name>
{
  use update;
  use delete;

  use association _Item { with draft; }
}